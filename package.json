{
  "name": "gosnel-landing-pages",
  "version": "1.0.0",
  "description": "GoSnel Landing Pages with Tailwind CSS Templates",
  "scripts": {
    "dev": "npm run build && node dev-server.js",
    "dev:watch": "concurrently \"tailwindcss -i ./src/input.css -o ./dist/output.css --watch\" \"npm run build:pages -- --watch\"",
    "build": "npm run build:css && npm run build:pages",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "build:pages": "node build.js",
    "serve": "npx http-server dist -p 3000 -c-1",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.1.0",
    "http-server": "^14.1.1",
    "postcss": "^8.4.32",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.6"
  },
  "keywords": [
    "tailwindcss",
    "landing-pages",
    "startup",
    "saas",
    "gosnel"
  ],
  "author": "GoSnel Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0"
  }
}
